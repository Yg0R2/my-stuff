<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="prototype" class="yg0r2.spring.core.BaseBean" scope="prototype">
		<property name="message" value="Prototype bean message." />
	</bean>

	<bean id="singleton" class="yg0r2.spring.core.BaseBean" scope="singleton">
		<property name="message" value="Singleton bean message." />
	</bean>

</beans>